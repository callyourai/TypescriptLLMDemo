{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",  // Update this line
      "moduleResolution": "node",  // Add this line
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["./src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  